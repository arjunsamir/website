{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "anime": [
        "node_modules/@juliangarnierorg/anime-beta"
      ],
      "@extradope": [
        "node_modules/@extra-dope/web-ui/lib/index.ts"
      ],
      "@extradope/*": [
        "node_modules/@extra-dope/web-ui/lib/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}